[user]
	name = TAKEDA-Takashi
	email = takeda.takashi0308@gmail.com
[http]
	sslverify = false
[core]
	excludesfile = ~/.gitignore_global
	editor = /usr/bin/vim
	quotepath = false
[init]
	templatedir = ~/.git_template
[alias]
	diffc = diff --cached
	logg = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
	br = browse
	delf = ! git branch | grep feature | xargs git branch -d
